      },
      "mirrorProperties": {},
      "calculationProperties": {},
      "relations": {

      }
    },
    {
      "identifier": "githubWorkflow",
      "title": "GitHub Workflow",
      "icon": "GitHub",
      "schema": {
        "properties": {
          "url": {
            "title": "Workflow URL",
            "type": "string",
            "format": "url"
          },
          "runUrl": {
            "title": "Run URL",
            "type": "string",
            "format": "url"
          },
          "status": {
            "title": "Status",
            "type": "string",
            "enum": ["queued", "in_progress", "completed"],
            "enumColors": {
              "queued": "yellow",
              "in_progress": "blue",
              "completed": "green"
            }
          },
          "conclusion": {
            "title": "Conclusion",
            "type": "string",
            "enum": ["success", "failure", "neutral", "cancelled", "skipped", "timed_out", "action_required"],
            "enumColors": {
              "success": "green",
              "failure": "red",
              "neutral": "gray",
              "cancelled": "gray",
              "skipped": "yellow",
              "timed_out": "red",
              "action_required": "orange"
            }
          },
          "headBranch": {
            "title": "Head Branch",
            "type": "string"
          },
          "headSha": {
            "title": "Head SHA",
            "type": "string"
          },
          "runNumber": {
            "title": "Run Number",
            "type": "number"
          },
          "event": {
            "title": "Trigger Event",
            "type": "string"
          },
          "createdAt": {
            "title": "Created At",
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "title": "Updated At",
            "type": "string",
            "format": "date-time"
          },
          "runStartedAt": {
            "title": "Run Started At",
            "type": "string",
            "format": "date-time"
          }
        },
        "required": ["url", "status", "repository"]
      },
      "mirrorProperties": {},
      "calculationProperties": {
        "statusBadge": {
          "title": "Status Badge",
          "calculation": "return `<span class=\"badge ${properties.conclusion === 'success' ? 'badge-success' : properties.conclusion === 'failure' ? 'badge-error' : 'badge-warning'}\">${properties.conclusion || properties.status}</span>`",
          "type": "string"
        }
      },
      "relations": {
         "target": "githubRepository",
         "title": "Repository",
         "description": "The repository containing this workflow",
         "required": false,
         "many": false
      }
  }
]