          "enumColors": {
            "active": "green",
            "deleted": "red"
          }
        },
        "createdAt": {
          "title": "Created At",
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "title": "Updated At",
          "type": "string",
          "format": "date-time"
        },
        "deletedAt": {
          "title": "Deleted At",
          "type": "string",
          "format": "date-time"
        },
        "link": {
          "title": "Link",
          "type": "string",
          "format": "url"
        }
      },
      "required": []
    },
    "mirrorProperties": {},
    "calculationProperties": {},
    "relations": {
      "repository": {
        "title": "Repository",
        "target": "githubRepository",
        "required": false,
        "many": false
      }
    }
  },
