        "target": "githubRepository",
        "title": "Repository",
        "description": "The repository this pull request belongs to",
        "required": false,
        "many": false
      }
    },
    {
      "identifier": "githubTeam",
      "title": "GitHub Team",
      "icon": "GitHub",
      "schema": {
        "properties": {
          "url": {
            "title": "URL",
            "type": "string",
            "format": "url"
          },
          "description": {
            "title": "Description",
            "type": "string"
          },
          "privacy": {
            "title": "Privacy",
            "type": "string",
            "enum": ["secret", "closed", "open"]
          },
          "organization": {
            "title": "Organization",
            "type": "string"
          },
          "createdAt": {
            "title": "Created At",
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "title": "Updated At",
            "type": "string",
            "format": "date-time"
          },
          "permission": {
            "title": "Permission",
            "type": "string",
            "enum": ["pull", "push", "admin"]
          }
