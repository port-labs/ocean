[
  {
    "identifier": "githubRepository",
    "title": "GitHub Repository",
    "icon": "GitHub",
    "description": "A repository on GitHub",
    "schema": {
      "properties": {
        "htmlUrl": {
          "title": "HTML URL",
          "type": "string",
          "format": "url",
          "description": "The GitHub web URL for this repository"
        },
        "fullName": {
          "title": "Full Name",
          "type": "string",
          "description": "Owner/Repo combination, e.g., 'RetailloopAfrica/testing-repo-1'"
        },
        "description": {
          "title": "Description",
          "type": "string",
          "description": "Repo description"
        },
        "private": {
          "title": "Private",
          "type": "boolean",
          "description": "Whether the repository is private"
        },
        "visibility": {
          "title": "Visibility",
          "type": "string",
          "description": "The repository visibility (e.g., 'public' or 'private')"
        },
        "defaultBranch": {
          "title": "Default Branch",
          "type": "string",
          "description": "The default branch name"
        },
        "language": {
          "title": "Language",
          "type": "string",
          "description": "Primary programming language"
        },
        "size": {
          "title": "Size",
          "type": "number",
          "description": "Repository size"
        },
        "archived": {
          "title": "Archived",
          "type": "boolean",
          "description": "Whether the repo is archived"
        },
        "oldName": {
          "title": "Old Name",
          "type": "string",
          "description": "Previous repo name if it was renamed"
        },
        "createdAt": {
          "title": "Created At",
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the repo was created"
        },
        "updatedAt": {
          "title": "Updated At",
          "type": "string",
          "format": "date-time",
          "description": "Timestamp when the repo was last updated"
        }
      }
    },
    "calculationProperties": {},
    "mirrorProperties": {},
    "aggregationProperties": {},
    "relations": {}
  },
  {
      "identifier": "githubPullRequest",
      "title": "GitHub Pull Request",
      "icon": "GitHub",
      "schema": {
        "properties": {
          "url": {
            "title": "URL",
            "type": "string",
            "format": "url"
          },
          "state": {
            "title": "State",
            "type": "string",
            "enum": ["open", "closed"],
            "enumColors": {
              "open": "green",
              "closed": "red"
            }
          },
          "repo": {
            "title": "Repository",
            "type": "string"
          },
          "author": {
            "title": "Author",
            "type": "string"
          },
          "number": {
            "title": "PR Number",
            "type": "number"
          },
          "createdAt": {
            "title": "Created At",
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "title": "Updated At",
            "type": "string",
            "format": "date-time"
          },
          "base": {
            "title": "Base Branch",
            "type": "string"
          },
          "head": {
            "title": "Head Branch",
            "type": "string"
          },
          "merged": {
            "title": "Merged",
            "type": "boolean"
          },
          "draft": {
            "title": "Draft",
            "type": "boolean"
          }
        },
        "required": ["url", "state", "repo", "author"]
      },
      "mirrorProperties": {},
      "relations": {
        "target": "githubRepository",
        "title": "Repository",
        "description": "The repository this pull request belongs to",
        "required": false,
        "many": false

      }
    },
    {
      "identifier": "githubIssue",
