            "enumColors": {
              "open": "green",
              "closed": "red"
            }
          },
          "repo": {
            "title": "Repository",
            "type": "string"
          },
          "number": {
            "title": "Issue Number",
            "type": "number"
          },
          "author": {
            "title": "Author",
            "type": "string"
          },
          "labels": {
            "title": "Labels",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "createdAt": {
            "title": "Created At",
