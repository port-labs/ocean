{
  "identifier": "my-production-bucket",
  "title": "my-production-bucket",
  "blueprint": "s3Bucket",
  "properties": {
    "arn": "arn:aws:s3:::my-production-bucket",
    "region": "us-west-2",
    "creationDate": "2023-10-15T14:30:00.000Z",
    "encryption": {
      "Rules": [
        {
          "ApplyServerSideEncryptionByDefault": {
            "SSEAlgorithm": "aws:kms",
            "KMSMasterKeyID": "arn:aws:kms:us-west-2:123456789012:key/12345678-1234-1234-1234-123456789012"
          },
          "BucketKeyEnabled": true
        }
      ]
    },
    "publicAccessBlockConfiguration": {
      "BlockPublicAcls": true,
      "IgnorePublicAcls": true,
      "BlockPublicPolicy": true,
      "RestrictPublicBuckets": true
    },
    "tags": [
      {
        "Key": "Environment",
        "Value": "production"
      },
      {
        "Key": "Team",
        "Value": "platform"
      },
      {
        "Key": "CostCenter",
        "Value": "engineering"
      }
    ]
  }
}
