            "CRITICAL",
            "BLOCKER"
          ],
          "enumColors": {
            "MAJOR": "orange",
            "INFO": "green",
            "CRITICAL": "red",
            "BLOCKER": "red",
            "MINOR": "yellow"
          }
        },
        "link": {
          "type": "string",
          "format": "url",
          "icon": "Link",
          "title": "Link"
        },
        "status": {
          "type": "string",
          "title": "Status",
          "enum": [
            "OPEN",
            "CLOSED",
