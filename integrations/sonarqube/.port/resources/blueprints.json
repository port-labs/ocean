          "type": "string",
          "title": "Branch",
          "icon": "GitVersion"
        },
        "fixedIssues": {
          "type": "number",
          "title": "Fixed Issues"
        },
        "newIssues": {
          "type": "number",
          "title": "New Issues"
        },
        "coverage": {
          "title": "Coverage",
          "type": "number"
        },
        "duplications": {
          "type": "number",
          "title": "Duplications"
        },
        "createdAt": {
          "type": "string",
