OCEAN__PORT__CLIENT_ID=<port_client_id>
OCEAN__PORT__CLIENT_SECRET=<port_client_secret>
OCEAN__INTEGRATION__IDENTIFIER=vercel
OCEAN__PORT__BASE_URL=https://api.getport.io
OCEAN__EVENT_LISTENER__TYPE=POLLING
OCEAN__INITIALIZE_PORT_RESOURCES=true

# ── Vercel credentials ─────────────────────────────────────────────────────────
# Your Vercel API token — generate at https://vercel.com/account/settings/tokens
OCEAN__INTEGRATION__CONFIG__TOKEN=your_vercel_api_token_here

# (Optional) Vercel team slug or ID to scope syncs to one team.
# Leave blank to sync your personal account.
OCEAN__INTEGRATION__CONFIG__TEAM_ID=your-team-slug-here

# (Optional) HMAC-SHA1 secret for validating incoming Vercel webhook payloads.
# Set the same value in your Vercel webhook settings.
OCEAN__INTEGRATION__CONFIG__WEBHOOK_SECRET=your_webhook_secret_here
