[
  {
    "identifier": "jfrogRepository",
    "title": "JFrog Repository",
    "icon": "JfrogXray",
    "schema": {
      "properties": {
        "key": { "type": "string", "title": "Key" },
        "description": { "type": "string", "title": "Description" },
        "type": {
          "type": "string",
          "title": "Repository Type",
          "enum": ["LOCAL", "REMOTE", "VIRTUAL", "FEDERATED", "DISTRIBUTION"]
        },
        "url": { "type": "string", "format": "url", "title": "URL" },
        "packageType": { "type": "string", "title": "Package Type" }
      }
    },
    "relations": {}
  },
  {
    "identifier": "jfrogBuild",
    "title": "JFrog Build",
    "icon": "JfrogXray",
    "schema": {
      "properties": {
        "name": { "type": "string", "title": "Build Name" },
        "uri": { "type": "string", "title": "Build URI" },
        "lastStarted": {
          "type": "string",
          "format": "date-time",
          "title": "Last Started"
        }
      }
    },
    "relations": {}
  },
  {
    "identifier": "jfrogProject",
    "title": "JFrog Project",
    "icon": "JfrogXray",
    "schema": {
      "properties": {
        "key": { "type": "string", "title": "Key" },
        "name": { "type": "string", "title": "Name" },
        "description": { "type": "string", "title": "Description" },
        "adminPrivileges": { "type": "object", "title": "Admin Privileges" },
        "roles": { "type": "array", "title": "Roles" }
      }
    },
    "relations": {}
  },
  {
    "identifier": "jfrogContainerImage",
    "title": "Container Image",
    "icon": "Docker",
    "schema": {
      "properties": {
        "name": { "type": "string", "title": "Image Name" },
        "tag": { "type": "string", "title": "Tag" },
        "fullName": { "type": "string", "title": "Full Name" },
        "sha256": { "type": "string", "title": "SHA256" },
        "imageSize": { "type": "string", "title": "Image Size" },
        "architecture": { "type": "string", "title": "Architecture" },
        "os": { "type": "string", "title": "Operating System" },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "title": "Created At"
        },
        "lastScanned": {
          "type": "string",
          "format": "date-time",
          "title": "Last Scanned"
        },
        "vulnerabilityCount": {
          "type": "number",
          "title": "Total Vulnerabilities"
        },
        "criticalVulns": {
          "type": "number",
          "title": "Critical Vulnerabilities"
        },
        "highVulns": { "type": "number", "title": "High Vulnerabilities" },
        "mediumVulns": { "type": "number", "title": "Medium Vulnerabilities" },
        "lowVulns": { "type": "number", "title": "Low Vulnerabilities" }
      },
      "required": ["name", "tag", "fullName"]
    },
    "relations": {
      "repository": {
        "target": "jfrogRepository",
        "required": false,
        "many": false
      }
    }
  },
  {
    "identifier": "jfrogContainerVulnerability",
    "title": "Container Vulnerability",
    "icon": "Vulnerability",
    "schema": {
      "properties": {
        "cve": { "type": "string", "title": "CVE" },
        "cwe": { "type": "array", "title": "CWE" },
        "severity": {
          "type": "string",
          "title": "Severity",
          "enum": [
            "Critical",
            "High",
            "Medium",
            "Low",
            "Informational",
            "Unknown"
          ]
        },
        "status": {
          "type": "string",
          "title": "Status",
          "enum": ["Open", "Fixed", "Ignored", "Not Applicable"],
          "default": "Open"
        },
        "component": { "type": "string", "title": "Component" },
        "imageName": { "type": "string", "title": "Image Name" },
        "imageTag": { "type": "string", "title": "Image Tag" },
        "cvssScore": { "type": "number", "title": "CVSS Score" },
        "description": {
          "type": "string",
          "format": "markdown",
          "title": "Description"
        },
        "summary": { "type": "string", "title": "Summary" },
        "created": {
          "type": "string",
          "format": "date-time",
          "title": "Created"
        },
        "issueId": { "type": "string", "title": "Issue ID" },
        "provider": { "type": "string", "title": "Provider" },
        "artifactPath": { "type": "string", "title": "Artifact Path" },
        "packageType": { "type": "string", "title": "Package Type" }
      },
      "required": ["cve", "severity", "status", "component", "imageName"]
    },
    "relations": {
      "containerImage": {
        "target": "jfrogContainerImage",
        "required": false,
        "many": false
      }
    }
  },
  {
    "identifier": "baseImageVulnerability",
    "title": "Base Image Vulnerability",
    "icon": "Vulnerability",
    "schema": {
      "properties": {
        "cve": { "type": "string", "title": "CVE" },
        "cwe": { "type": "array", "title": "CWE" },
        "severity": {
          "type": "string",
          "title": "Severity",
          "enum": [
            "Critical",
            "High",
            "Medium",
            "Low",
            "Informational",
            "Unknown"
          ]
        },
        "status": {
          "type": "string",
          "title": "Status",
          "enum": ["Open", "Fixed", "Ignored", "Not Applicable"],
          "default": "Open"
        },
        "baseImage": { "type": "string", "title": "Base Image" },
        "layer": { "type": "string", "title": "Container Layer" },
        "component": { "type": "string", "title": "Component" },
        "fixedVersion": { "type": "string", "title": "Fixed Version" },
        "cvssScore": { "type": "number", "title": "CVSS Score" },
        "description": {
          "type": "string",
          "format": "markdown",
          "title": "Description"
        },
        "summary": { "type": "string", "title": "Summary" },
        "created": {
          "type": "string",
          "format": "date-time",
          "title": "Created"
        },
        "issueId": { "type": "string", "title": "Issue ID" },
        "provider": { "type": "string", "title": "Provider" }
      },
      "required": ["cve", "severity", "status", "baseImage"]
    },
    "relations": {
      "containerImage": {
        "target": "jfrogContainerImage",
        "required": false,
        "many": false
      }
    }
  }
]
