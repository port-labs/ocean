                        "malware",
                        "eol",
                        "scm_security",
                        "ai_pentest",
                        "license"
                    ],
                    "enumColors": {
                        "open_source": "turquoise",
                        "leaked_secret": "orange",
                        "cloud": "blue",
                        "iac": "darkGray",
                        "sast": "green",
                        "surface_monitoring": "bronze",
                        "malware": "red",
                        "eol": "purple",
                        "scm_security": "yellow",
                        "ai_pentest": "pink",
                        "license": "bronze"
                    },
                    "description": "Issue group category."
                },
                "severity_score": {
                    "type": "number",
                    "title": "Severity Score",
                    "description": "A number indicating the importance of the vulnerability"
                },
