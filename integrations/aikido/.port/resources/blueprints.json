                    "enumColors": {
                        "new": "purple",
                        "todo": "yellow",
                        "task_open": "orange",
                        "task_closed": "green",
                        "pull_request_open": "blue"
                    },
                    "description": "Workflow status of the issue group."
                },
                "severity": {
                    "type": "string",
                    "title": "Severity",
                    "enum": ["critical", "high", "medium", "low"],
                    "enumColors": {
                        "critical": "red",
                        "high": "orange",
                        "medium": "yellow",
                        "low": "green"
                    },
                    "description": "A human friendly version of the issue's importance"
                },
                "locations": {
                    "items": {
                        "type": "object"
                    },
                    "title": "Locations",
                    "type": "array"
                },
                "priority": {
                    "type": "number",
                    "title": "Priority",
                    "description": "Priority score assigned to the group."
                },
                "time_to_fix_minutes": {
                    "type": "number",
                    "title": "Time To Fix (Minutes)",
                    "description": "The estimated time in minutes that it will take to fix this issue."
                },
                "how_to_fix": {
                    "type": "string",
                    "title": "How To Fix",
                    "description": "Recommended remediation guidance."
                }
            },
            "required": []
        },
        "relations": {
            "aikido_repositories": {
                "title": "Repositories",
                "target": "aikido_repository_ocean_poc",
                "required": false,
                "many": true
            }
        }
    }
   
]
