[
    {
        "identifier": "aikido_repository_ocean_poc",
        "title": "Aikido Repository",
        "icon": "Aikido",
        "schema": {
            "properties": {
                "provider": {
                    "type": "string",
                    "title": "Provider",
                    "enum": ["github", "gitlab", "gitlab-server", "bitbucket", "azure_devops", "selfscan"],
                    "enumColors": {
                        "github": "darkGray",
                        "gitlab": "orange",
                        "gitlab-server": "purple",
                        "bitbucket": "blue",
                        "azure_devops": "turquoise",
                        "selfscan": "lightGray"
                    },
                    "description": "The place where the code repository is hosted."
                },
                "external_repository_id": {
                    "type": "string",
                    "title": "External Repository ID",
                    "description": "The id of the repository from the provider."
                },
                "active": {
                    "type": "boolean",
                    "title": "Active",
                    "description": "Whether the repository is actively being scanned."
                },
                "external_repository_url": {
                    "type": "string",
                    "title": "Repository URL",
                    "format": "url",
                    "description": "External URL to the repository."
                },
                "branch": {
                    "type": "string",
                    "title": "Scanned Branch",
                    "description": "The branch of the repository that Aikido will scan."
                },
                "last_scan_at": {
                    "icon": "ClockLoader",
                    "type": "string",
                    "format": "date-time",
                    "title": "Last Scan At",
                    "description": "The last time a scan for this repository ended."
                }
            },
            "required": []
        },
        "relations": {}
    },
    {
        "identifier": "aikido_container_ocean_poc",
        "title": "Aikido Container",
        "icon": "Aikido",
        "schema": {
            "properties": {
                "provider": {
                    "type": "string",
                    "title": "Provider",
                    "enum": [
                        "aws",
                        "aws_ecr",
                        "gcp-artifact-registry",
                        "gcp-container-registry",
                        "docker-hub",
                        "github",
                        "quay",
                        "acr",
                        "gitlab",
                        "gitlab-self",
                        "digitalocean",
                        "scaleway",
                        "cloudsmith",
                        "harbor",
                        "artifactory",
                        "custom_upload",
                        "oci"
                    ],
                    "enumColors": {
                        "aws": "orange",
                        "aws_ecr": "orange",
                        "gcp-artifact-registry": "blue",
                        "gcp-container-registry": "blue",
                        "docker-hub": "turquoise",
                        "github": "darkGray",
                        "quay": "purple",
                        "acr": "lightGray",
                        "gitlab": "orange",
                        "gitlab-self": "purple",
                        "digitalocean": "blue",
                        "scaleway": "pink",
                        "cloudsmith": "turquoise",
                        "harbor": "green",
                        "artifactory": "yellow",
                        "custom_upload": "bronze",
                        "oci": "darkGray"
                    },
                    "description": "The provider where the container repository is hosted."
                },
                "cloud_id": {
                    "type": "number",
                    "title": "Cloud ID",
                    "description": "Cloud identifier where this container registry is located."
                },
                "registry_id": {
                    "type": "number",
                    "title": "Registry ID",
                    "description": "The container registry identifier."
                },
                "registry_name": {
                    "type": "string",
                    "title": "Registry Name",
                    "description": "The name of the container registry."
                },
                "tag": {
                    "type": "string",
                    "title": "Tag Filter",
                    "description": "The tag filter used when selecting an image. If empty the latest image will be scanned."
                },
                "distro": {
                    "type": "string",
                    "title": "Distribution",
                    "description": "The distribution of the image operating system."
                },
                "distro_version": {
                    "type": "string",
                    "title": "Distribution Version",
                    "description": "The distribution version of the image operating system."
                },
                "last_scanned_at": {
                    "type": "string",
                    "format": "date-time",
                    "title": "Last Scanned At",
                    "description": "The latest completed scan."
                },
                "last_scanned_tag": {
                    "type": "string",
                    "title": "Last Scanned Tag",
                    "description": "The image tag used in the latest scan."
                },
                "last_pushed_at": {
                    "type": "string",
                    "format": "date-time",
                    "title": "Last Pushed At",
                    "description": "The latest push to this container repository."
                }
            },
            "required": []
        },
        "relations": {
            "aikido_repository": {
                "title": "Repository",
                "target": "aikido_repository_ocean_poc",
                "required": false,
                "many": false
            }
        }
    },
     {
        "identifier": "aikido_team_ocean_poc",
        "title": "Aikido Team",
        "icon": "Aikido",
        "schema": {
            "properties": {
                "external_source": {
                    "type": "string",
