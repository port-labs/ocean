                    "enum": ["github", "gitlab", "bitbucket", "backstage_io"],
                    "enumColors": {
                        "github": "darkGray",
                        "gitlab": "orange",
                        "bitbucket": "blue",
                        "backstage_io": "turquoise"
                    },
                    "description": "Indicates if this team is linked to an external source."
                },
                "external_source_id": {
                    "type": "string",
                    "title": "External Source ID",
                    "description": "The external source identifier for this team."
                },
                "responsibilities": {
                    "type": "array",
                    "title": "Responsibilities",
                    "items": {
                        "type": "object"
                    },
                    "description": "Resources this team is responsible for."
                },
                "active": {
                    "type": "boolean",
                    "title": "Active",
                    "description": "Whether the team is currently active."
                }
            },
            "required": []
        },
        "relations": {
