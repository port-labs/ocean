                "title": "Containers",
                "target": "aikido_container_ocean_poc",
                "required": false,
                "many": true
            }
        }
    },
    {
        "identifier": "aikido_issue_ocean_poc",
        "title": "Aikido Issue",
        "icon": "Aikido",
        "schema": {
            "properties": {
                "affected_file": {
                    "type": "string",
                    "title": "Affected File",
                    "description": "The name of affected file, if any."
                },
                "affected_package": {
                    "type": "string",
                    "title": "Affected Package",
                    "description": "The name of affected open-source package, if any."
                },
                "attack_surface": {
                    "title": "Attack Surface",
                    "description": "The attack surface of this issue.",
                    "type": "string",
                    "default": "not_applicable",
                    "enum": [
                        "backend",
                        "frontend",
                        "infrastructure",
                        "docker_container",
                        "cloud",
                        "not_applicable"
                    ],
                    "enumColors": {
                        "backend": "brown",
                        "frontend": "pink",
                        "infrastructure": "darkGray",
                        "docker_container": "blue",
                        "cloud": "lightGray",
                        "not_applicable": "red"
                    }
                },
                "closed_at": {
                    "type": "string",
                    "title": "Closed At",
                    "format": "date-time",
                    "description": "When the issue was closed/resolved."
                },
                "code_repo_id": {
                    "type": "string",
                    "title": "Code Repository ID",
                    "description": "The id of the code repository where this issue was found."
                },
                "code_repo_name": {
                    "type": "string",
                    "title": "Code Repository Name",
                    "description": "The name of code repository where this issue was found."
                },
                "container_repo_name": {
                    "type": "string",
                    "title": "Container Repository Name",
                    "description": "The name of the container image where this issue was found."
                },
                "cve_id": {
                    "type": "string",
                    "title": "CVE ID",
                    "description": "CVE ID in case of an issue of type open_source."
                },
                "cwe_ids": {
                    "type": "array",
                    "title": "CWE Classes",
                    "items": {
                        "type": "string"
                    },
                    "description": "A list of CWE classes that apply to this issue."
                },
                "detected_at": {
                    "type": "string",
                    "title": "First Detected At",
                    "format": "date-time"
                },
                "group_id": {
                    "type": "number",
                    "title": "Group ID"
                },
                "ignored_at": {
                    "type": "string",
                    "title": "Ignored At",
                    "format": "date-time"
                },
                "remediate_by": {
                    "type": "string",
                    "title": "Remediate By",
                    "format": "date-time"
                },
                "rule": {
                    "type": "string",
                    "title": "Rule Name"
                },
                "severity": {
                    "type": "string",
                    "title": "Severity",
                    "enum": ["critical", "high", "medium", "low"],
                    "enumColors": {
                        "critical": "red",
                        "high": "orange",
                        "medium": "yellow",
                        "low": "green"
                    }
                },
                "severity_score": {
                    "type": "number",
                    "title": "Severity Score"
                },
                "sla_days": {
                    "type": "number",
                    "title": "SLA (Days)"
                },
                "snoozed_until": {
                    "type": "string",
                    "title": "Snoozed Until",
                    "format": "date-time"
                },
                "status": {
                    "type": "string",
                    "title": "Status",
                    "enum": ["open", "closed", "ignored", "snoozed"],
                    "enumColors": {
                        "open": "red",
                        "closed": "green",
                        "ignored": "yellow",
                        "snoozed": "blue"
                    }
                },
                "type": {
                    "icon": "DefaultProperty",
                    "title": "Issue Type",
                    "type": "string",
                    "enum": [
                        "open_source",
                        "leaked_secret",
                        "cloud",
                        "iac",
                        "sast",
                        "mobile",
                        "surface_monitoring",
                        "malware",
                        "eol",
                        "scm_security",
                        "ai_pentest",
                        "license"
                    ],
                    "enumColors": {
                        "open_source": "turquoise",
                        "leaked_secret": "orange",
                        "cloud": "blue",
                        "iac": "darkGray",
                        "sast": "green",
                        "mobile": "lime",
                        "surface_monitoring": "olive",
                        "malware": "red",
                        "eol": "purple",
                        "scm_security": "yellow",
                        "ai_pentest": "pink",
                        "license": "gold"
                    }
                }
            },
            "required": ["status", "severity", "type"]
        },
        "relations": {
            "aikido_repository": {
                "title": "Repository",
                "target": "aikido_repository_ocean_poc",
                "required": false,
                "many": false
            }
        }
    },
    {
