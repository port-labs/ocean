# OAuth credentials for Port integration
OCEAN__PORT__CLIENT_ID="<port-client-id>"  # Unique client ID for authentication
OCEAN__PORT__CLIENT_SECRET="<port-client-secret>"  # Secret key for client authentication

# Base URLs for Ocean and Port API communication
OCEAN__BASE_URL=https://test.com  # Base URL for the Ocean API
OCEAN__PORT__BASE_URL=https://api.getport.io  # Base URL for the Port API

# Integration-specific settings
OCEAN__INTEGRATION__IDENTIFIER=bitbucket-cloud  # Identifier for the Bitbucket Cloud integration
OCEAN__EVENT_LISTENER__TYPE=POLLING  # Defines how events are captured (POLLING or WEBHOOK)
OCEAN__INITIALIZE_PORT_RESOURCES=true  # Whether to initialize resources in Port upon startup

# Bitbucket integration configuration
OCEAN__INTEGRATION__CONFIG__BITBUCKET_WORKSPACE=port_ocean  # Default Bitbucket workspace
OCEAN__INTEGRATION__CONFIG__BITBUCKET_BASE_URL=https://api.bitbucket.org/2.0  # Base API URL for Bitbucket
OCEAN__INTEGRATION__CONFIG__WEBHOOK_SECRET=4542832c1ed418e8  # Secret for validating incoming Bitbucket webhooks

# Application host and credentials
OCEAN__INTEGRATION__CONFIG__APP_HOST="<app-host>"  # Host URL where the integration is running
OCEAN__INTEGRATION__CONFIG__USERNAME="<username>"  # Bitbucket username for API access
OCEAN__INTEGRATION__CONFIG__APP_PASSWORD="<app-password>"  # Bitbucket app password for authentication
